// // // // // // //             result[i][j]=0;
// // // // // // //             for (int k = 0; k < x; k++)
// // // // // // //             {
// // // // // // //                 result[i][j]+=a[i][k]*b[k][j];
// // // // // // //             }
            
// // // // // // //         }
        
// // // // // // //     }

// // // // // // //     for ( i = 0; i < x; i++)
// // // // // // //     {
