            //BINARY TO DECIMAL
#include<stdio.h>
#include<time.h>
#include<string.h>
#include<stdlib.h>
#include<math.h>
#include<conio.h>
#include<windows.h>


int main(){

    int j,i=0,bin,dec;
    int sum=0;

    printf("Enter a binary number:");
    scanf("%d",&bin);

    while (bin>0)
    {
      
        int remainder=bin%10;
        sum+=remainder*pow(2,i);
          i++;
          bin=bin/10;
    }
    printf("The decimal number is %d \n",sum);

    getch();

    return 0;
}
